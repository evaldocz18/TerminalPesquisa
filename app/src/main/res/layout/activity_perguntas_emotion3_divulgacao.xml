<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/papel_paredel"
    tools:context=".activity.perguntas.PerguntasEmotion3DivulgacaoActivity"
    tools:layout_editor_absoluteY="89dp">

    <ImageView
        android:id="@+id/iv_divulgacao"
        android:layout_width="0dp"
        android:layout_height="500dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toTopOf="@+id/iv_sugestoes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_pergunta3emotion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="Text View nÃ£o carregou a pergunta"
        android:padding="8dp"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_excelente"
        android:layout_width="615dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:onClick="clickExelente3Emotion"
        android:padding="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_pergunta3emotion"
        app:srcCompat="@drawable/excelente_emotion"
        tools:ignore="OnClick" />

    <ImageView
        android:id="@+id/iv_razoavel"
        android:layout_width="610dp"
        android:layout_height="120dp"
        android:layout_marginStart="3dp"
        android:layout_marginLeft="3dp"
        android:layout_marginTop="8dp"
        android:onClick="clickRazoavel3Emotion"
        android:padding="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_excelente"
        app:srcCompat="@drawable/medio_emotion" />

    <ImageView
        android:id="@+id/iv_ruim"
        android:layout_width="610dp"
        android:layout_height="120dp"
        android:layout_marginTop="8dp"
        android:onClick="clickRuim3Emotion"
        android:padding="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_razoavel"
        app:srcCompat="@drawable/pessimo_emotion" />

    <ImageView
        android:id="@+id/iv_elogios"
        android:layout_width="180dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="8dp"
        android:onClick="onclickElogios"
        android:src="@drawable/elogios"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/iv_sugestoes"
        android:layout_width="180dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:onClick="onclickSugestoes"
        android:src="@drawable/sugestoes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/iv_reclamacoes"
        app:layout_constraintStart_toEndOf="@+id/iv_elogios" />

    <ImageView
        android:id="@+id/iv_reclamacoes"
        android:layout_width="180dp"
        android:layout_height="120dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:onClick="onclickReclamacoes"
        android:src="@drawable/reclamacoes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>